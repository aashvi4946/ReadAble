{
    "name": "ReadAble",
    "version": "1.0.0",
    "author": "Anuja Pandey",
    "description": "A dyslexia friendly Chrome Extension",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_title": "ReadAble"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}